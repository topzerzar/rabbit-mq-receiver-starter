{
  "name": "rabbitmq-pumping-service",
  "version": "0.1.0",
  "description": "Receive data from rabbitMQ and Manage data",
  "main": "server.js",
  "scripts": {
    "start": "./node_modules/.bin/gulp start",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --colors test/**",
    "test:unit": "NODE_ENV=test ./node_modules/.bin/mocha --colors test/unit",
    "test:integration": "NODE_ENV=test ./node_modules/.bin/mocha --colors test/integration",
    "test:system": "NODE_ENV=test ./node_modules/.bin/mocha --colors test/system",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/gulp test",
    "test:coverage": "NODE_ENV=test ./node_modules/.bin/istanbul cover -x '**/config/**' -x '**/routes.js' -x '**/server.js' ./node_modules/.bin/_mocha test/**",
    "apidoc:watch": "./node_modules/.bin/gulp apidoc",
    "apidoc": "./node_modules/.bin/gulp genapidoc"
  },
  "author": "weerapon",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.5.1",
    "axios": "^0.15.3",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "clone": "^1.0.2",
    "co": "^4.6.0",
    "co-express": "^1.2.1",
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "forever": "^0.15.2",
    "helmet": "^2.1.1",
    "json-api-ify": "^1.1.1",
    "json-api-serializer": "^1.1.0",
    "method-override": "^2.3.6",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "request": "^2.75.0",
    "url": "^0.11.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.7.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-apidoc": "^0.2.5",
    "gulp-nodemon": "^2.2.1",
    "gulp-shell": "^0.5.2",
    "istanbul": "0.4.5",
    "mocha": "^3.2.0",
    "nodemon": "^1.10.2",
    "rewire": "^2.5.2",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2",
    "supertest": "^2.0.0"
  }
